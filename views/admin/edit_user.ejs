<form
  action="/docs/admin/save_edit_user?_method=PUT"
  method="POST"
  class="d-inline"
>
  <!-- id -->

  <div class="mb-3">
    <label for="email" class="form-label">Email address</label>
    <input
      type="email"
      class="form-control"
      name="email"
      value="<%= userTarget.email %>"
    />
  </div>

  <div class="mb-3">
    <label for="userName" class="form-label">Username</label>
    <input
      name="userName"
      type="text"
      class="form-control"
      value="<%= userTarget.userName %>"
    />
  </div>
  <div class="mb-3">
    <label for="fullName" class="form-label">Full Name</label>
    <input
      type="text"
      name="fullName"
      class="form-control"
      value="<%= userTarget.fullName %>"
    />
  </div>
  <input type="hidden" name="id" value="<%= userTarget.id %>" />
  <div class="mb-3">
    <label for="role" class="form-label">Role</label>
    <input
      type="text"
      name="role"
      type="number"
      class="form-control"
      value="<%=userTarget.role%>"
    />
  </div>

  <div class="mb-3">
    <label for="apikey" class="form-label">Apikey</label>
    <input
      type="text"
      name="apikey"
      class="form-control"
      value="<%= userTarget.apikey %>"
    />
  </div>
  <!-- <a href=""> -->
  <button type="submit" class="btn btn-primary">Save</button>
  <!-- delete -->

  <!-- </a> -->
</form>
<form
  id="form-delete-data"
  action="/docs/admin/list_user/delete?_method=DELETE"
  method="POST"
  class="d-inline"
>
  <input type="hidden" name="idTarget" value="<%- userTarget.id %>" />
  <button type="submit" class="btn btn-outline-danger">Delete</button>
</form>
